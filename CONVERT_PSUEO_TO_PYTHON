if b == int(1):
   c = int(input("Enter an integer:"))
   print(c), print("INT is same in peudocode and python!")
elif b == int(2):
    c = float(input("Enter a number:"))
    print(c), print("REAL is equal to FLOAT in python!")
elif b == int(3):
    c = str(input("Enter data:"))
    print(c), print("Python doesn't have CHAR but its equal to STRING!")
elif b == int(4):
    c = str(input("Enter anything!:")
    print("c"), print("STRING in psuedocode is always within double brackets!)
elif b == int(5):
    print("Is 5 greater than 6? False! Its not"), print("Boolean is the same in psuedocode and python, they have two outputs, true or false!)
elif b == int(6):
    now = datetime.datetime.now()
