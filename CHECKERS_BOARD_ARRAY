n = int(input())
m = int(input())

q = []

for i in range(n):
    p=[]
    if i % 2 == 0:
        for j in range (m):
            if j % 2 == 0:
                l = "."
            else:
                l = "*"
            p.append(l)
    else:
        for j in range (m):
            if j % 2 == 0:
                l = "*"
            else:
                l = "."
            p.append(l)  
    q.append(p)
            
print(q)
